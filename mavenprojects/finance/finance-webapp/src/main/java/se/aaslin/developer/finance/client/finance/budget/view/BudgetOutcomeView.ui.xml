<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

	<ui:style field="styles">
		.panel {
			float: left;
		}
		
		.listBox {
			margin-left: 85px;
		}
	</ui:style>

	<g:HTMLPanel styleName="{styles.panel}">
		<g:ListBox ui:field="listBox" styleName="{styles.listBox}" />
		<g:HTMLPanel ui:field="chartPanel" />
		<g:HTMLPanel ui:field="tablePanel" />
	</g:HTMLPanel>
</ui:UiBinder>
